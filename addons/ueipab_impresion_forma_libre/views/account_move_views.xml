<odoo>
  <data>
    <record id="inherited_account_move_form_inherit_ueipab_impresion_forma_libre" model="ir.ui.view">
      <field name="name">account.move.view.form.inherit</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="impresion_forma_libre.inherited_account_move_form" />
      <field name="arch" type="xml">
        <field name="fiscal_check" position="replace">
          <field name="fiscal_check" widget="boolean_toggle"
            invisible="('move_type','in',['entry', 'out_invoice', 'in_invoice'])"/>
            <!-- attrs="{'invisible': [('move_type','in',['entry', 'out_receipt', 'in_receipt'])]}"  -->
        </field>
        <field name="control_number" position="replace">
          <field name="control_number"
             invisible="('move_type','in',['entry', 'out_invoice', 'in_invoice'])"/>
             <!-- attrs="{'invisible': [('move_type','in',['entry', 'out_receipt', 'in_receipt'])]}" -->
        </field>
        <field name="fiscal_correlative" position="replace">
          <field name="fiscal_correlative"
             invisible="('move_type','in',['entry', 'out_invoice', 'in_invoice'])"/>
             <!-- attrs="{'invisible': [('move_type','in',['entry', 'out_receipt', 'in_receipt'])]}" -->
        </field>
      </field>
    </record>
  </data>
</odoo>