<?xml version="1.0" encoding="utf-8"?>
<template id="template" xml:space="preserve">
    <t t-name="tdv_multi_currency_pos.Orderline" t-inherit="point_of_sale.Orderline" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('product-price')]" position="before">
            <div class="product-price me-4 d-inline-block text-end price fw-bolder">
                <t t-out="line.secondPrice"/>
            </div>
        </xpath>
    </t>
</template>