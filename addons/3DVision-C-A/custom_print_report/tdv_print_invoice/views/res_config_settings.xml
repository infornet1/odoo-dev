<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="res_config_settings_view_form_inherit_custom_print" model="ir.ui.view">
    <field name="name">res.config.settings.view.form.inherit.custom.print</field>
    <field name="model">res.config.settings</field>
    <field name="inherit_id" ref="account.res_config_settings_view_form"/>
    <field name="arch" type="xml">
      <block id="invoicing_settings" position="inside">
          <setting id="custom_invoice_setting" help="Enable custom print format for invoice">
              <field name="enable_custom_invoice_format"/>
          </setting>
      </block>
    </field>
  </record>
</odoo>
